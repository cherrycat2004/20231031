      "source": [
        "def minOperation(boxes):\n",
        "  n = len(boxes)\n",
        "  answer = [0] * n\n",
        "  for i in range(n):\n",
        "    for j in range(n):\n",
        "      if boxes[j] == '1':\n",
        "        answer[i] += abs(i - j)\n",
        "  return answer\n",
        "\n",
        "a = input('輸入：')\n",
        "print(minOperation(list(a)))"
      ]
    }
  ]
